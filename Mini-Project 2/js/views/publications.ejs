<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <meta http-equiv="x-ua-compatible" content="ie=edge" />
    <title>Publications</title>
    <!-- MDB icon -->
    <link rel="icon" href="img/mdb-favicon.ico" type="image/x-icon" />
    <!-- Font Awesome -->
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css"
    />
    <!-- Google Fonts Roboto -->
    <link
      rel="stylesheet"
      href="https://fonts.googleapis.com/css2?family=Roboto:wght@300;400;500;700;900&display=swap"
    />
    <!-- MDB -->
    <link rel="stylesheet" href="/css/mdb.min.css" />
    <!-- Custom CSS -->
    <link rel="stylesheet" href="/css/publications.css">
  </head>
  <body>
    <!-- Start your project here-->
    <!-- Navbar -->
  <nav class="navbar navbar-expand-lg navbar-light bg-light custom-navbar"
  style="background-color: #363979 !important; color: white;">
  <!-- Container wrapper -->
  <div class="container-fluid">
    <!-- Toggle button -->
    <button class="navbar-toggler custom-navbar-toggle-button" id="custom-navbar-toggle-button" type="button" data-mdb-toggle="collapse" data-mdb-target="#navbarSupportedContent"
      aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
      <i class="fas fa-bars"></i>
    </button>

    <!-- Collapsible wrapper -->
    <div class="collapse navbar-collapse" id="navbarSupportedContent">
      <!-- Navbar brand -->
      <a class="navbar-brand mt-2 mt-lg-0" href="#">
        <img src="/img/collegeLogo.png" class="college-logo" alt="MDB Logo" loading="lazy" />
      </a>
      <button class="openbtn custom-open-button" onclick="openNav()" id="openbtn"><span class="custom-open-button-hamburger">&#9776;</span></button>
      <span class="navbar-text-span">Publications Management System - MIT WPU</span>
    </div>
    <!-- Collapsible wrapper -->

    <!-- Right elements -->
    <div class="d-flex align-items-center">
      <!-- Avatar -->
      <div class="dropdown">
        <a class="dropdown-toggle d-flex align-items-center hidden-arrow" href="#" id="navbarDropdownMenuAvatar"
          role="button" data-mdb-toggle="dropdown" aria-expanded="false">
          <img src="/img/avatar.png" class="rounded-circle" height="40" alt="Black and White Portrait of a Man"
            loading="lazy" />
        </a>
      </div>
    </div>
    <!-- Right elements -->
  </div>
  <!-- Container wrapper -->
</nav>
<!-- Navbar -->

    <!-- Side Navigation Bar -->
    <div id="mySidebar" class="sidebar">
        <!-- <a href="javascript:void(0)" id="closebtn class="closebtn" onclick="closeNav()">Branches&times;</a> -->
        <!-- <center><span class="custom-branch-topic">Faculty</span></center>
        <center><span class="custom-branch-topic">Member</span></center>
        <center><img src="./img/prof.png" class="prof-photo custom-sidenav-links my-3" id="prof-photo" alt=""></center>
        <center><span class="custom-branch-topic">Prof.Varsha Naik</span></center>
        <center><span class="custom-branch-topic">Faculty at school of Engineering</span></center> -->
        <!-- <center><a href="#" class="btn btn-light custom-sidenav-links my-3">Faculty at School and Engineering</a></center> -->
      </div>
      
      <div id="main">
        <!-- <button class="openbtn" id="openbtn" onclick="openNav()">&#9776; Open Sidebar</button>
        <h2>Collapsed Sidebar</h2> -->

        <!-- content -->
        <div class="container ">
          <div class="topic">
            <center><span class="main-topic">Publications</span></center>
          </div>
          <div class="branches-name my-5">
            <span class="sub-sub-topic"><h2>Conference Paper</h2></span><br>
            <ul>
              <% conferencePaper.forEach(function( eachPaper){ %>
                <li> Title : <%= eachPaper.title %> <br> Domain : <%= eachPaper.domain %> </li>
            <% }) %>
            </ul> <br>
          <span class="sub-sub-topic"><h2>Book Chapter</h2></span><br>
          <ul>
            <% bookChapters.forEach(function( eachChapter){ %>
              <li> Title : <%= eachChapter.book_title %> </li>
          <% }) %>
          </ul> <br>
          <span class="sub-sub-topic"><h2>Journals</h2></span><br>
          <ul>
            <% journals.forEach(function( eachJournal){ %>
              <li> Title : <%= eachJournal.title %>  <br> Journal Name : <%= eachJournal.journal_name %> </li>
          <% }) %>
          </ul> 
          </div>
        </div>
      </div>
      <!-- End of side navigation bar -->

    <!-- End your project here-->

    <!-- MDB -->
    <script type="text/javascript" src="/mdb.min.js"></script>
    <!-- Custom scripts -->
    <script src="/publications.js"></script>
  </body>
</html>
